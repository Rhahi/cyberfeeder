{
  "manifest_version": 2,
  "name": "Cyberfeeder",
  "version": "3.5.0",
  "description": "UI improvements for jinteki.net",
  "icons": {
    "16": "icons/JINTEKI_16.png",
    "32": "icons/JINTEKI_32.png",
    "48": "icons/JINTEKI_48.png",
    "96": "icons/JINTEKI_96.png",
    "128": "icons/JINTEKI_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.jinteki.net/*",
        "https://*.reteki.fun/*",
        "http://jinteki.local/*"
      ],
      "js": [
        "js/jnet.js"
      ]
    }
  ],
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "downloads"
  ],
  "sidebar_action": {
    "default_icon": {
      "16": "icons/JINTEKI_16.png",
      "32": "icons/JINTEKI_32.png",
      "48": "icons/JINTEKI_48.png",
      "96": "icons/JINTEKI_96.png",
      "128": "icons/JINTEKI_128.png"
    },
    "default_title": "Cyberfeeder",
    "default_panel": "html/sidebar.html",
    "open_at_install": false
  },
  "page_action": {
    "default_icon": {
      "16": "icons/JINTEKI_16.png",
      "32": "icons/JINTEKI_32.png",
      "48": "icons/JINTEKI_48.png",
      "96": "icons/JINTEKI_96.png",
      "128": "icons/JINTEKI_128.png"
    },
    "default_title": "Cyberfeeder",
    "show_matches": [
      "https://*.jinteki.net/*",
      "https://*.reteki.fun/*",
      "http://jinteki.local/*"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/JINTEKI_16.png",
      "32": "icons/JINTEKI_32.png",
      "48": "icons/JINTEKI_48.png",
      "96": "icons/JINTEKI_96.png",
      "128": "icons/JINTEKI_128.png"
    },
    "default_title": "Open Sidebar"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "webfonts/fa-solid-900.woff2",
    "css/fontawesome.min.css",
    "css/solid.min.css"
  ]
}
